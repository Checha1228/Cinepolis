.cont-btnG
	height 45px
	text-align center
	
.btns-home
	background-color $GrisOs
	border none
	box-shadow 0 2px 0 0 $Negro
	color $Amarillo
	font-family $KBlanKS
	text-transform uppercase
	height 35px
	width 100%
	transitionBase(0.10s)
	
	&:hover
		background-color $AzulOs
		transform translateY(-5px)
		box-shadow 1px 4px 0px $Negro

.btns-registros
	background-color $Azul
	border none
	box-shadow 0 2px 0 0 $Negro
	color $Blanco
	font-family $KBlanKS
	text-transform uppercase
	height 35px
	width 100%
	transitionBase(0.10s)
	
	&:hover
		background-color $AzulOs
		color $Amarillo
		transform translateY(-5px)
		box-shadow 1px 4px 0px $Negro

.btns-texto-solid
	
		font-family $KBlanKS
		font-size 5.5rem
		background-color rgba(0, 0, 0, 0)
		color $Blanco
		border none
		text-transform uppercase
	